#include <stdio.h>

int main()
{
    printf("Hello World \n");
    char word [] = "cat";
    char name [6] = "Brian";

    printf("%s \n", word);
    printf("%s \n", name);

    int i;
    int array2[10];
    //asigning loop
    for (i=0; i<10; i++){
        array2[i]= i*i;
    }
    //print loop
    for (i=0; i<10; i++){
        printf("%d\n", array2[i]);
    }
    return 0;
}
